{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/Bridgeconn/usfm-grammar/blob/master/schemas/FlatJSON.js",
  "title": "USFM-Grammar JSON(Nested)",
  "description": "Parsed output of USFM file, with a flat structure",
  "type": "object",
  "properties":{
    "book":{
      description: "The contents of an entire bible book or a USFM file",
      type: "object",
      properties:{
        "id":{
          type:"object",
          properties:{
            "bookCode":{
              type:"string",
              enum:["GEN", "EXO", "LEV", "NUM", "DEU", "JOS", "JDG",
              "RUT", "1SA", "2SA", "1KI", "2KI", 
              "1CH", "2CH", "EZR", "NEH", "EST", "JOB", "PSA", "PRO", 
              "ECC", "SNG", "ISA", "JER", "LAM", "EZK", "DAN", 
              "HOS", "JOL", "AMO", "OBA", "JON", "MIC", "NAM", "HAB", 
              "ZEP", "HAG", "ZEC", "MAL", "MAT", "MRK", "LUK", 
              "JHN", "ACT", "ROM", "1CO", "2CO", "GAL", "EPH", "PHP", 
              "COL", "1TH", "2TH", "1TI", "2TI", "TIT", "PHM", 
              "HEB", "JAS", "1PE", "2PE", "1JN", "2JN", "3JN", "JUD", 
              "REV", "TOB", "JDT", "ESG", "WIS", "SIR", "BAR", 
              "LJE", "S3Y", "SUS", "BEL", "1MA", "2MA", "3MA", "4MA", 
              "1ES", "2ES", "MAN", "PS2", "ODA", "PSS", "EZA", 
              "5EZ", "6EZ", "DAG", "PS3", "2BA", "LBA", "JUB", "ENO", 
              "1MQ", "2MQ", "3MQ", "REP", "4BA", "LAO", "FRT", 
              "BAK", "OTH", "INT", "CNC", "GLO", "TDX", "NDX"]
            },
            "fileDescription":{type:"string"}
          },
          required:["bookCode"],
          additionalProperties:false
        },
        "headers":{
          type:"array",
          items:{ "$ref":"#/$defs/marker-or-array"}
        },
        "chapters":{
          type:"array",
          contains:{
            type:"object",
            properties:{
              "c":{type:"string"},
              "ca":{type:"string"},
              "cp":{type:"string"},
              "cl":{type:"string"},
              "cd":{type:"string"},
              "contents":{
                type:"array",
                items:{"$ref":"#/$defs/para-title-or-section"}
              }
            },
            additionalProperties:false,
            required:["c", "contents"]
          }
        }
      },
      additionalProperties:false,
      required:["id"]
    }
  },
  required:["book"],
  additionalProperties:false,
  "$defs":{

    "para-title-or-section":{
      oneOf:[
        {
          type:"object", 
          patternProperties:{
            "^(p|m|po|pr|cls|pmo|pm|pmc|pmr|pi\\d?|mi|pc|ph\\d?|phi)$":{
              type:"array",
              items:{"$ref":"#/$defs/text-marker-or-array"}
            }
          }, 
          properties:{
            "list":{
              type:"array",
                items:{ "$ref": "#/$defs/marker"}
            },
            "poetry":{
              type:"array",
              items:{ "$ref": "#/$defs/marker"}
            },
            "table":{
              type:"array",
                items:{ 
                  type: "object",
                  properties:{
                    "tr":{type: "array", items:{
                      type:"object",
                      patternProperties:{
                        "^t(c|cr|h|hr)\\d?$":{
                          type:"array",
                          items:{"$ref":"#/$defs/text-or-marker"}
                        }
                      },
                      additionalProperties:false
                    }}
                  },
                  additionalProperties:false
                }
            }
          },
          maxProperties:1, 
          additionalProperties:false
        },
        {type:"array",items:{"$ref":"#/$defs/marker"}}
      ]
    },


    "text-marker-or-array":{
      oneOf:[
        {type:"string"},
        {"$ref":"#/$defs/marker"},
        {type:"array",items:{"$ref":"#/$defs/marker"}}
      ]
    },

    "marker-or-array":{
      oneOf:[
        {"$ref":"#/$defs/marker"},
        {type:"array",items:{"$ref":"#/$defs/marker"}}
      ]
    },

    "text-or-marker":{
      oneOf:[
        {type:"string"},
        {"$ref":"#/$defs/marker"},
      ]
    },

    "marker":{
